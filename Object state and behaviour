public class Person {
  
  int age;
  String name;
  
  
  public void set_value() 
  {
    age = 20;
    name = "Robin";
  }
  public void get_value() {
    System.out.println("Age is " + age);
    System.out.println("Name is " + name);
  }
  
 
  public static void main(String [] args) {
    // creates a new Person object
    Person p = new Person(); 
    
    
    p.set_value();
  }
}
