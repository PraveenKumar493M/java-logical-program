// 1. Write a Java program to convert temperature from Fahrenheit to Celsius degree. (ip-212,,,ans... 100)
import java.util.Scanner;
class Celsius {
    public static void main(String[] args)
    {
        Scanner input= new Scanner(System.in);
        System.out.println("Enter the Fahrenheit :");
        int temp= input.nextInt();
        int output= (int) ((temp-32) *(0.5556));
        System.out.println(output+" degree celsius ");
    }
}
